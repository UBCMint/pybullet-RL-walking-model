<?xml version="1.0" ?>
<robot name="quadruped_robot">
  <!-- Define the main body -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.6 0.3 0.1"/> <!-- Main body dimensions (length x width x height) -->
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.6 0.3 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
  </link>

  <!-- Front Left Leg -->
  <!-- Prismatic Joint for vertical movement -->
  <joint name="front_left_prismatic" type="prismatic">
    <parent link="base_link"/>
    <child link="front_left_attachment_point"/>
    <origin xyz="0.2 0.15 0" rpy="0 0 0"/> <!-- Fixed attachment to the body -->
    <axis xyz="0 0 1"/> <!-- Vertical movement along z-axis -->
    <limit lower="0.0" upper="0.2" effort="50.0" velocity="1.0"/> <!-- Limits for the prismatic joint -->
  </joint>

  <!-- Intermediate link for front left leg attachment -->
  <link name="front_left_attachment_point">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05"/> <!-- Link between prismatic and rotational joints -->
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>

  <!-- Continuous Joint for rotational movement -->
  <joint name="front_left_rotational" type="continuous">
    <parent link="front_left_attachment_point"/>
    <child link="front_left_leg"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/> <!-- Rotational movement along y-axis -->
    <limit effort="5.0" velocity="1.0"/>
  </joint>

  <!-- Front Left Leg Link -->
  <link name="front_left_leg">
    <visual>
      <geometry>
        <cylinder length="0.4" radius="0.05"/> <!-- Leg length -->
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Front Right Leg -->
  <!-- Prismatic Joint for vertical movement -->
  <joint name="front_right_prismatic" type="prismatic">
    <parent link="base_link"/>
    <child link="front_right_attachment_point"/>
    <origin xyz="0.2 -0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0.0" upper="0.2" effort="50.0" velocity="1.0"/>
  </joint>

  <!-- Intermediate link for front right leg attachment -->
  <link name="front_right_attachment_point">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>

  <!-- Continuous Joint for rotational movement -->
  <joint name="front_right_rotational" type="continuous">
    <parent link="front_right_attachment_point"/>
    <child link="front_right_leg"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" velocity="1.0"/>
  </joint>

  <!-- Front Right Leg Link -->
  <link name="front_right_leg">
    <visual>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Back Left Leg -->
  <!-- Prismatic Joint for vertical movement -->
  <joint name="back_left_prismatic" type="prismatic">
    <parent link="base_link"/>
    <child link="back_left_attachment_point"/>
    <origin xyz="-0.2 0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0.0" upper="0.2" effort="50.0" velocity="1.0"/>
  </joint>

  <!-- Intermediate link for back left leg attachment -->
  <link name="back_left_attachment_point">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>

  <!-- Continuous Joint for rotational movement -->
  <joint name="back_left_rotational" type="continuous">
    <parent link="back_left_attachment_point"/>
    <child link="back_left_leg"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" velocity="1.0"/>
  </joint>

  <!-- Back Left Leg Link -->
  <link name="back_left_leg">
    <visual>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

  <!-- Back Right Leg -->
  <!-- Prismatic Joint for vertical movement -->
  <joint name="back_right_prismatic" type="prismatic">
    <parent link="base_link"/>
    <child link="back_right_attachment_point"/>
    <origin xyz="-0.2 -0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0.0" upper="0.2" effort="50.0" velocity="1.0"/>
  </joint>

  <!-- Intermediate link for back right leg attachment -->
  <link name="back_right_attachment_point">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>

  <!-- Continuous Joint for rotational movement -->
  <joint name="back_right_rotational" type="continuous">
    <parent link="back_right_attachment_point"/>
    <child link="back_right_leg"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" velocity="1.0"/>
  </joint>

  <!-- Back Right Leg Link -->
  <link name="back_right_leg">
    <visual>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

</robot>
