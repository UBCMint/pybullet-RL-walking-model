<?xml version="1.0" ?>
<robot name="quadruped_robot">
  <!-- Define the main body -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.6 0.3 0.1"/> <!-- Main body dimensions (length x width x height) -->
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.6 0.3 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
  </link>

  <!-- Define each leg -->
  <!-- Front Left Leg -->
  <link name="front_left_leg">
    <visual>
      <geometry>
        <cylinder length="0.2" radius="0.05"/> <!-- Leg dimensions -->
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Joint connecting front left leg to the body -->
  <joint name="front_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_left_leg"/>
    <origin xyz="0.2 0.15 0" rpy="0 0 0"/> <!-- Position of the joint relative to the body -->
    <axis xyz="0 1 0"/> <!-- Rotation axis -->
    <limit effort="5.0" velocity="1.0"/>
  </joint>

  <!-- Front Right Leg -->
  <link name="front_right_leg">
    <visual>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Joint connecting front right leg to the body -->
  <joint name="front_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_right_leg"/>
    <origin xyz="0.2 -0.15 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" velocity="1.0"/>
  </joint>

  <!-- Back Left Leg -->
  <link name="back_left_leg">
    <visual>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Joint connecting back left leg to the body -->
  <joint name="back_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="back_left_leg"/>
    <origin xyz="-0.2 0.15 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" velocity="1.0"/>
  </joint>

  <!-- Back Right Leg -->
  <link name="back_right_leg">
    <visual>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
  </link>

  <!-- Joint connecting back right leg to the body -->
  <joint name="back_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="back_right_leg"/>
    <origin xyz="-0.2 -0.15 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" velocity="1.0"/>
  </joint>
</robot>
